<?xml version="1.0" encoding="utf-8"?>
<s:BorderContainer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				   xmlns:s="library://ns.adobe.com/flex/spark" 
				   xmlns:mx="library://ns.adobe.com/flex/mx" 
				   width="36" height="36" borderColor="0x999999" 
				   borderStyle="solid" borderWeight="1" cornerRadius="4">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:layout>
		<s:HorizontalLayout paddingTop="2" paddingLeft="2" paddingBottom="2" 
							paddingRight="4" verticalAlign="middle" horizontalAlign="center" />
	</s:layout>
	
	<mx:Image source="/assets/images/house.png" width="26" height="26" 
			  smoothBitmapContent="true" alpha="{this.enabled ? 1 : 0.3}" />
	
</s:BorderContainer>
